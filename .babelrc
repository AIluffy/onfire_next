{
	"presets": [
		[
			"next/babel",
			{
				"styled-jsx": {
					"plugins": [
						[
							"styled-jsx-plugin-postcss",
							{
								"path": "[PATH_PREFIX]/postcss.config.js"
							}
						],
						"styled-jsx-plugin-sass"
					]
				}
			}
		]
	],
	"plugins": [
		[
			"@babel/plugin-proposal-decorators",
			{
				"legacy": true
			}
		]
	]
}
